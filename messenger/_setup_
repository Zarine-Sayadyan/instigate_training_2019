
# THIS FILE IS READONLY !!!
#
# This file can be changed by entering
# "make setup use_sdk=<use_sdk> use_gcov=<use_gcov>
#	build_type=<build_type> link_type=<link_type>
#	sdk_path=<sdk_path> 
#	office_sdk_path=<office_sdk_path>"
# command from command line.
# <build_type> must be either 'debug' or 'release'
# <link_type> must be either 'dynamic' or 'static'
# <use_sdk> must be either 'yes' or 'no'
# <use_gcov> must be either 'yes' or 'no'

export build_type:=debug
export link_type:=dynamic
export use_sdk:=no
export use_gcov:=no
